{% extends "base.html" %}
{% block page_subtitle %} {{ issue.title }} {% endblock %}

{% block content %}

{% if issue %}

<h3> {{ issue.title }} </h3>

  <ul>
    {% for issue.article in issue.articles %}
      <li>{{issue.article.id}}</li>
    {% endfor %}
  </ul>


  <h4> {{issue.article_title}} </h4>
  <p> {{issue.article.id}} </p>
  <p> {{issue.article_author}} </p>
  <p> {{issue.article_type}} </p>
  <p> {{issue.article_pages}} </p>
  <p> {{issue.article_num}} </p>
  <p> {{issue.article_id}} </p>
  <p> {{issue.nextdiv}} </p>


  

{{issue.article}}
    
{% endif %}
 
{% endblock %}